{
  "version": 2,
  "name": "bird-bones-website",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/comments",
      "dest": "/api/comments.js"
    },
    {
      "src": "/(.*\\.(?:jpg|jpeg|png|gif|svg|css|js|woff|woff2|eot|ttf))",
      "dest": "/$1"
    },
    {
      "src": "^/(?!api/)",
      "dest": "/api/index.js"
    }
  ]
}
